{
    "name": "reactjs-hooks-typescript-practice",
    "version": "0.0.1",
    "private": false,
    "dependencies": {
      "@ant-design/icons": "^4.1.0",
      "@types/hookrouter": "^2.2.1",
      "@umijs/hooks": "^1.9.3",
      "antd": "^4.4.1",
      "axios": "^0.19.2",
      "hookrouter": "^1.2.3",
      "less": "^3.11.1",
      "react": "^16.13.1",
      "react-dom": "^16.13.1",
      "react-hooks-testing-library": "^0.6.0",
      "react-scripts": "3.4.1"
    },
    "scripts": {
      "start": "react-app-rewired start",
      "build": "react-app-rewired build",
      "type-checks":"tsc",
      "test": "react-app-rewired test --env=jest-environment-jsdom-sixteen",
      "eject": "react-scripts eject",
      "lint": "eslint --max-warnings 0  --ext .js,.jsx,.ts,.tsx ./src"
    },
    "eslintConfig": {
      "extends": "react-app"
    },
    "jest": {
      "moduleNameMapper": {
        "@/(.*)": "<rootDir>/src/$1",
        "\\.(css|less)$": "identity-obj-proxy"
      },
      "setupFilesAfterEnv": []
    },
    "browserslist": {
      "production": [
        ">0.2%",
        "not dead",
        "not op_mini all"
      ],
      "development": [
        "last 1 chrome version",
        "last 1 firefox version",
        "last 1 safari version"
      ]
    },
    "devDependencies": {
      "@testing-library/dom": "^7.20.0",
      "@testing-library/jest-dom": "^5.11.0",
      "@testing-library/react": "^10.4.0",
      "@testing-library/react-hooks": "^3.2.1",
      "@testing-library/user-event": "^12.0.0",
      "@types/jest": "^24.0.0",
      "@types/lodash": "^4.14.151",
      "@types/node": "^12.0.0",
      "@types/react": "^16.9.0",
      "@types/react-dom": "^16.9.0",
      "@typescript-eslint/eslint-plugin": "^2.30.0",
      "@typescript-eslint/parser": "^2.30.0",
      "babel-plugin-import": "^1.13.0",
      "customize-cra": "^1.0.0",
      "eslint": "^6.8.0",
      "eslint-config-prettier": "^6.11.0",
      "eslint-import-resolver-alias": "^1.1.2",
      "eslint-plugin-import": "^2.20.2",
      "eslint-plugin-node": "^11.1.0",
      "eslint-plugin-prettier": "^3.1.3",
      "eslint-plugin-promise": "^4.2.1",
      "eslint-plugin-react": "^7.19.0",
      "eslint-plugin-react-hooks": "^4.0.2",
      "eslint-plugin-simple-import-sort": "^5.0.3",
      "http-proxy-middleware": "^1.0.3",
      "husky": "^4.2.5",
      "identity-obj-proxy": "^3.0.0",
      "jest-environment-jsdom-sixteen": "^1.0.3",
      "less-loader": "^5.0.0",
      "lodash": "^4.17.15",
      "prettier": "2.0.5",
      "react-app-rewired": "^2.1.5",
      "react-test-renderer": "^16.13.1",
      "typescript": "~3.8.0",
      "typescript-plugin-css-modules": "^2.3.0"
    },
    "husky": {
      "hooks": {
        "pre-commit": "yarn lint && yarn type-checks"
      }
    }
  }
  